	measure sessions_LTD =
			
			VAR CurrentYear = YEAR(TODAY())
			VAR CurrentMonth = MONTH(TODAY())
			
			VAR CurrentYTD =
			    CALCULATE(
			        [sessions],
			        DATESYTD('Z - DateTable'[Date])
			    )
			
			VAR LaunchYTD =
			    CALCULATE(
			        [sessions],
			        DATESYTD('Z - DateTable'[Date]),
			        FILTER(
			            ALL('Z - DateTable'),
			            MONTH('Z - DateTable'[Date]) = 3 &&
			            YEAR('Z - DateTable'[Date]) = 2025
			        )
			    )
			
			RETURN
			    IF(
			        CurrentYear < 2025 || (CurrentYear = 2025 && CurrentMonth < 3),
			        BLANK(),
			        FORMAT(DIVIDE(CurrentYTD - LaunchYTD, LaunchYTD), "0.00%")
			    )

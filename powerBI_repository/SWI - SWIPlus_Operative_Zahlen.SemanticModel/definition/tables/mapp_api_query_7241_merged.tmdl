	measure Average_Weekly_Visits_By_Month = ```
			
			VAR WeeklyVisitsTable =
			    ADDCOLUMNS(
			        SUMMARIZE(
			            'Z - DateTable',
			            'Z - DateTable'[WeekMidWednesday]
			        ),
			        "WeekVisits",
			        CALCULATE(
			            DISTINCTCOUNT('mapp_api_query_7241_merged'[session_id]),
			            FILTER(
			                'mapp_api_query_7241_merged',
			                'mapp_api_query_7241_merged'[visit_date] >= 'Z - DateTable'[WeekMidWednesday] - 3
			                &&
			                'mapp_api_query_7241_merged'[visit_date] <= 'Z - DateTable'[WeekMidWednesday] + 3
			            )
			        ),
			        "WeekMonth",
			        FORMAT('Z - DateTable'[WeekMidWednesday], "YYYY-MM")
			    )
			
			VAR SelectedMonth = SELECTEDVALUE('Z - DateTable'[Month])
			
			RETURN
			    AVERAGEX(
			        FILTER(
			            WeeklyVisitsTable,
			            [WeekMonth] = SelectedMonth
			        ),
			        [WeekVisits]
			    )
			
			```
		formatString: 0
		lineageTag: 8d9e490d-058f-4750-ae91-b895ba89d89a

	measure Visits_Last24Months_2 = ```
			
			VAR ReferenceDate =
			    MAX ( 'Z - DateTable'[Date] )
			VAR Last24Months =
			    DATESINPERIOD (
			        'Z - DateTable'[Date],
			        ReferenceDate,
			        -25,
			        MONTH
			    )
			
			-- gleiche Weekly-Tabelle wie zuvor, aber nur im Zeitfenster
			VAR WeeklyVisitsTable =
			    ADDCOLUMNS (
			        FILTER (
			            SUMMARIZE ( 'Z - DateTable', 'Z - DateTable'[WeekMidWednesday] ),
			            'Z - DateTable'[WeekMidWednesday] IN Last24Months
			        ),
			        "WeekVisits",
			            CALCULATE (
			                DISTINCTCOUNT ( 'mapp_api_query_7241_merged'[session_id] ),
			                DATESBETWEEN (
			                    'Z - DateTable'[Date],
			                    'Z - DateTable'[WeekMidWednesday] - 3,
			                    'Z - DateTable'[WeekMidWednesday] + 3
			                )
			            ),
			        "WeekMonth",
			            FORMAT ( 'Z - DateTable'[WeekMidWednesday], "YYYY-MM" )
			    )
			RETURN
			    AVERAGEX ( WeeklyVisitsTable, [WeekVisits] )
			
			```
